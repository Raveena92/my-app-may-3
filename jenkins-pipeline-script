pipeline{
         agent any
         stages {
               stage('Preparation') { 
                     steps {
                             echo  "this is scm preparation stage"
                      }
                }
                stage('Build') { 
                     steps {
                             echo  "this is build stage 2"
                      }
                }
                stage('test') { 
                      steps {
                             echo  "this is test stage"
                      }
                }
                stage('Deploy') { 
                     steps {
                             echo  "this is deploy stage"
                      }
                }
        }
    }
